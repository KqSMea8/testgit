package com.hanweb.jmp.sys.controller.sites;

import java.io.Serializable;
import com.hanweb.complat.entity.User;
import com.hanweb.jmp.sys.entity.sites.Site;

public class SiteFormBean implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 4656689720419642801L;

	/**
	 * user
	 */
	private User user = new User();
	
	/**
	 * site
	 */
	private Site site = new Site();
	
	public Site getSite() {
		return site;
	}

	public void setSite(Site site) {
		this.site = site;
	}

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}
	
}