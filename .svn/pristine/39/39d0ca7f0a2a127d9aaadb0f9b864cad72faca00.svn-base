package com.hanweb.jmp.apps.controller.read;

import org.springframework.web.multipart.MultipartFile;

import com.hanweb.jmp.apps.entity.read.Read;

public class ReadFormBean extends Read implements java.io.Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 9151207700662531666L;

	/**
	 * 封面图片
	 */
	private MultipartFile picFile;
	
	/**
	 * 海报图片
	 */
	private MultipartFile bigFile;
	
	/**
	 * 文件
	 */
	private MultipartFile bookFile;

	public MultipartFile getPicFile() {
		return picFile;
	}

	public void setPicFile(MultipartFile picFile) {
		this.picFile = picFile;
	}

	public MultipartFile getBookFile() {
		return bookFile;
	}

	public void setBookFile(MultipartFile bookFile) {
		this.bookFile = bookFile;
	}

	public MultipartFile getBigFile() {
		return bigFile;
	}

	public void setBigFile(MultipartFile bigFile) {
		this.bigFile = bigFile;
	}

}