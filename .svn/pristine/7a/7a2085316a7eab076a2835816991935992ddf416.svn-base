	<script src="{sysPath}/resources/solicitopinion/js/mui.min.js"></script>
	<script src="{sysPath}/resources/solicitopinion/js/jquery-1.8.3.min.js"></script>
	<script>
		mui.init({
			swipeBack:true //启用右滑关闭功能
		});
		
